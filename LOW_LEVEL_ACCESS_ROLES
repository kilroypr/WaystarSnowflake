USE ROLE SECURITYADMIN; 
CREATE ROLE IF NOT EXISTS			      			    _QA_REMIT_DB_PREP_READONLY		  			     ; 
CREATE ROLE IF NOT EXISTS			      			    _QA_REMIT_DB_PREP_CRUD						     ; 
GRANT OWNERSHIP ON ROLE 			      			    _QA_REMIT_DB_PREP_READONLY TO ROLE useradmin; 
GRANT OWNERSHIP ON ROLE 			      			    _QA_REMIT_DB_PREP_CRUD 	TO ROLE useradmin; 
GRANT ROLE 							      			_QA_REMIT_DB_PREP_CRUD 	TO ROLE SYSADMIN	 ; 
GRANT ROLE 							      			_QA_REMIT_DB_PREP_READONLY TO ROLE SYSADMIN     ; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA QA_REMIT_DB.PREP TO ROLE _QA_REMIT_DB_PREP_READONLY; 
GRANT READ 	 ON ALL STAGES 	  		      			 IN SCHEMA QA_REMIT_DB.PREP TO ROLE _QA_REMIT_DB_PREP_READONLY; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA QA_REMIT_DB.PREP TO ROLE _QA_REMIT_DB_PREP_READONLY; 
GRANT SELECT ON FUTURE TABLES 		      			     IN SCHEMA QA_REMIT_DB.PREP TO ROLE _QA_REMIT_DB_PREP_READONLY; 
GRANT SELECT ON ALL VIEWS 	  		      			     IN SCHEMA QA_REMIT_DB.PREP TO ROLE _QA_REMIT_DB_PREP_READONLY; 
GRANT SELECT ON FUTURE VIEWS  		      			     IN SCHEMA QA_REMIT_DB.PREP TO ROLE _QA_REMIT_DB_PREP_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA QA_REMIT_DB.PREP TO ROLE _QA_REMIT_DB_PREP_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA QA_REMIT_DB.PREP TO ROLE _QA_REMIT_DB_PREP_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA QA_REMIT_DB.PREP TO ROLE _QA_REMIT_DB_PREP_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA QA_REMIT_DB.PREP TO ROLE _QA_REMIT_DB_PREP_READONLY; 
GRANT SELECT ON ALL EXTERNAL TABLES       			     IN SCHEMA QA_REMIT_DB.PREP TO ROLE _QA_REMIT_DB_PREP_READONLY; 
GRANT SELECT ON FUTURE EXTERNAL TABLES 	  			 IN SCHEMA QA_REMIT_DB.PREP TO ROLE _QA_REMIT_DB_PREP_READONLY; 
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON ALL TABLES    IN SCHEMA QA_REMIT_DB.PREP TO ROLE _QA_REMIT_DB_PREP_CRUD;     
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON FUTURE TABLES IN SCHEMA QA_REMIT_DB.PREP TO ROLE _QA_REMIT_DB_PREP_CRUD;     
GRANT ROLE _QA_REMIT_DB_PREP_READONLY to  role _QA_REMIT_DB_PREP_CRUD; 
USE ROLE ACCOUNTADMIN; 
GRANT USAGE ON DATABASE QA_REMIT_DB to ROLE _QA_REMIT_DB_PREP_READONLY; 
GRANT USAGE ON SCHEMA QA_REMIT_DB.PREP TO ROLE _QA_REMIT_DB_PREP_READONLY; 

USE ROLE SECURITYADMIN; 
CREATE ROLE IF NOT EXISTS			      			    _QA_REMIT_DB_ETL_READONLY		  			     ; 
CREATE ROLE IF NOT EXISTS			      			    _QA_REMIT_DB_ETL_CRUD						     ; 
GRANT OWNERSHIP ON ROLE 			      			    _QA_REMIT_DB_ETL_READONLY TO ROLE useradmin; 
GRANT OWNERSHIP ON ROLE 			      			    _QA_REMIT_DB_ETL_CRUD 	TO ROLE useradmin; 
GRANT ROLE 							      			_QA_REMIT_DB_ETL_CRUD 	TO ROLE SYSADMIN	 ; 
GRANT ROLE 							      			_QA_REMIT_DB_ETL_READONLY TO ROLE SYSADMIN     ; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA QA_REMIT_DB.ETL TO ROLE _QA_REMIT_DB_ETL_READONLY; 
GRANT READ 	 ON ALL STAGES 	  		      			 IN SCHEMA QA_REMIT_DB.ETL TO ROLE _QA_REMIT_DB_ETL_READONLY; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA QA_REMIT_DB.ETL TO ROLE _QA_REMIT_DB_ETL_READONLY; 
GRANT SELECT ON FUTURE TABLES 		      			     IN SCHEMA QA_REMIT_DB.ETL TO ROLE _QA_REMIT_DB_ETL_READONLY; 
GRANT SELECT ON ALL VIEWS 	  		      			     IN SCHEMA QA_REMIT_DB.ETL TO ROLE _QA_REMIT_DB_ETL_READONLY; 
GRANT SELECT ON FUTURE VIEWS  		      			     IN SCHEMA QA_REMIT_DB.ETL TO ROLE _QA_REMIT_DB_ETL_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA QA_REMIT_DB.ETL TO ROLE _QA_REMIT_DB_ETL_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA QA_REMIT_DB.ETL TO ROLE _QA_REMIT_DB_ETL_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA QA_REMIT_DB.ETL TO ROLE _QA_REMIT_DB_ETL_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA QA_REMIT_DB.ETL TO ROLE _QA_REMIT_DB_ETL_READONLY; 
GRANT SELECT ON ALL EXTERNAL TABLES       			     IN SCHEMA QA_REMIT_DB.ETL TO ROLE _QA_REMIT_DB_ETL_READONLY; 
GRANT SELECT ON FUTURE EXTERNAL TABLES 	  			 IN SCHEMA QA_REMIT_DB.ETL TO ROLE _QA_REMIT_DB_ETL_READONLY; 
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON ALL TABLES    IN SCHEMA QA_REMIT_DB.ETL TO ROLE _QA_REMIT_DB_ETL_CRUD;     
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON FUTURE TABLES IN SCHEMA QA_REMIT_DB.ETL TO ROLE _QA_REMIT_DB_ETL_CRUD;     
GRANT ROLE _QA_REMIT_DB_ETL_READONLY to  role _QA_REMIT_DB_ETL_CRUD; 
USE ROLE ACCOUNTADMIN; 
GRANT USAGE ON DATABASE QA_REMIT_DB to ROLE _QA_REMIT_DB_ETL_READONLY; 
GRANT USAGE ON SCHEMA QA_REMIT_DB.ETL TO ROLE _QA_REMIT_DB_ETL_READONLY; 

USE ROLE SECURITYADMIN; 
