GRANT READ 	 ON ALL STAGES 	  		      			 IN SCHEMA QA_REMIT_DB.PUBLIC TO ROLE _QA_REMIT_DB_PUBLIC_READONLY; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA QA_REMIT_DB.PUBLIC TO ROLE _QA_REMIT_DB_PUBLIC_READONLY; 
GRANT SELECT ON FUTURE TABLES 		      			     IN SCHEMA QA_REMIT_DB.PUBLIC TO ROLE _QA_REMIT_DB_PUBLIC_READONLY; 
GRANT SELECT ON ALL VIEWS 	  		      			     IN SCHEMA QA_REMIT_DB.PUBLIC TO ROLE _QA_REMIT_DB_PUBLIC_READONLY; 
GRANT SELECT ON FUTURE VIEWS  		      			     IN SCHEMA QA_REMIT_DB.PUBLIC TO ROLE _QA_REMIT_DB_PUBLIC_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA QA_REMIT_DB.PUBLIC TO ROLE _QA_REMIT_DB_PUBLIC_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA QA_REMIT_DB.PUBLIC TO ROLE _QA_REMIT_DB_PUBLIC_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA QA_REMIT_DB.PUBLIC TO ROLE _QA_REMIT_DB_PUBLIC_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA QA_REMIT_DB.PUBLIC TO ROLE _QA_REMIT_DB_PUBLIC_READONLY; 
GRANT SELECT ON ALL EXTERNAL TABLES       			     IN SCHEMA QA_REMIT_DB.PUBLIC TO ROLE _QA_REMIT_DB_PUBLIC_READONLY; 
GRANT SELECT ON FUTURE EXTERNAL TABLES 	  			 IN SCHEMA QA_REMIT_DB.PUBLIC TO ROLE _QA_REMIT_DB_PUBLIC_READONLY; 
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON ALL TABLES    IN SCHEMA QA_REMIT_DB.PUBLIC TO ROLE _QA_REMIT_DB_PUBLIC_CRUD;     
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON FUTURE TABLES IN SCHEMA QA_REMIT_DB.PUBLIC TO ROLE _QA_REMIT_DB_PUBLIC_CRUD;     
GRANT ROLE _QA_REMIT_DB_PUBLIC_READONLY to  role _QA_REMIT_DB_PUBLIC_CRUD; 
USE ROLE ACCOUNTADMIN; 
GRANT USAGE ON DATABASE QA_REMIT_DB to ROLE _QA_REMIT_DB_PUBLIC_READONLY; 
GRANT USAGE ON SCHEMA QA_REMIT_DB.PUBLIC TO ROLE _QA_REMIT_DB_PUBLIC_READONLY; 

