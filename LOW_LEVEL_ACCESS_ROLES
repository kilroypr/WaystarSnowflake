GRANT SELECT ON ALL EXTERNAL TABLES       			     IN SCHEMA NICK_DB.NEWUSERSCHEMA TO ROLE _NICK_DB_NEWUSERSCHEMA_READONLY; 
GRANT SELECT ON FUTURE EXTERNAL TABLES 	  			 IN SCHEMA NICK_DB.NEWUSERSCHEMA TO ROLE _NICK_DB_NEWUSERSCHEMA_READONLY; 
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON ALL TABLES    IN SCHEMA NICK_DB.NEWUSERSCHEMA TO ROLE _NICK_DB_NEWUSERSCHEMA_CRUD;     
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON FUTURE TABLES IN SCHEMA NICK_DB.NEWUSERSCHEMA TO ROLE _NICK_DB_NEWUSERSCHEMA_CRUD;     
GRANT ROLE _NICK_DB_NEWUSERSCHEMA_READONLY to  role _NICK_DB_NEWUSERSCHEMA_CRUD; 
USE ROLE ACCOUNTADMIN; 
GRANT USAGE ON DATABASE NICK_DB to ROLE _NICK_DB_NEWUSERSCHEMA_READONLY; 
GRANT USAGE ON SCHEMA NICK_DB.NEWUSERSCHEMA TO ROLE _NICK_DB_NEWUSERSCHEMA_READONLY; 

USE ROLE SECURITYADMIN; 
CREATE ROLE IF NOT EXISTS			      			    _NICK_DB_DEMO_READONLY		  			     ; 
CREATE ROLE IF NOT EXISTS			      			    _NICK_DB_DEMO_CRUD						     ; 
GRANT OWNERSHIP ON ROLE 			      			    _NICK_DB_DEMO_READONLY TO ROLE useradmin; 
GRANT OWNERSHIP ON ROLE 			      			    _NICK_DB_DEMO_CRUD 	TO ROLE useradmin; 
GRANT ROLE 							      			_NICK_DB_DEMO_CRUD 	TO ROLE SYSADMIN	 ; 
GRANT ROLE 							      			_NICK_DB_DEMO_READONLY TO ROLE SYSADMIN     ; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA NICK_DB.DEMO TO ROLE _NICK_DB_DEMO_READONLY; 
GRANT READ 	 ON ALL STAGES 	  		      			 IN SCHEMA NICK_DB.DEMO TO ROLE _NICK_DB_DEMO_READONLY; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA NICK_DB.DEMO TO ROLE _NICK_DB_DEMO_READONLY; 
GRANT SELECT ON FUTURE TABLES 		      			     IN SCHEMA NICK_DB.DEMO TO ROLE _NICK_DB_DEMO_READONLY; 
GRANT SELECT ON ALL VIEWS 	  		      			     IN SCHEMA NICK_DB.DEMO TO ROLE _NICK_DB_DEMO_READONLY; 
GRANT SELECT ON FUTURE VIEWS  		      			     IN SCHEMA NICK_DB.DEMO TO ROLE _NICK_DB_DEMO_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA NICK_DB.DEMO TO ROLE _NICK_DB_DEMO_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA NICK_DB.DEMO TO ROLE _NICK_DB_DEMO_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA NICK_DB.DEMO TO ROLE _NICK_DB_DEMO_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA NICK_DB.DEMO TO ROLE _NICK_DB_DEMO_READONLY; 
GRANT SELECT ON ALL EXTERNAL TABLES       			     IN SCHEMA NICK_DB.DEMO TO ROLE _NICK_DB_DEMO_READONLY; 
GRANT SELECT ON FUTURE EXTERNAL TABLES 	  			 IN SCHEMA NICK_DB.DEMO TO ROLE _NICK_DB_DEMO_READONLY; 
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON ALL TABLES    IN SCHEMA NICK_DB.DEMO TO ROLE _NICK_DB_DEMO_CRUD;     
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON FUTURE TABLES IN SCHEMA NICK_DB.DEMO TO ROLE _NICK_DB_DEMO_CRUD;     
GRANT ROLE _NICK_DB_DEMO_READONLY to  role _NICK_DB_DEMO_CRUD; 
USE ROLE ACCOUNTADMIN; 
GRANT USAGE ON DATABASE NICK_DB to ROLE _NICK_DB_DEMO_READONLY; 
GRANT USAGE ON SCHEMA NICK_DB.DEMO TO ROLE _NICK_DB_DEMO_READONLY; 

