/**
Create Low level access roles for the existing databases on the Waystar Snowflake

Create by: Miguel Rivera Caban
Created on: 2021-05-01
**/
USE ROLE SECURITYADMIN; 
CREATE ROLE IF NOT EXISTS			      			    _QA_BENCHMARKING_DB_PUBLIC_READONLY		  			     ; 
CREATE ROLE IF NOT EXISTS			      			    _QA_BENCHMARKING_DB_PUBLIC_CRUD						     ; 
GRANT OWNERSHIP ON ROLE 			      			    _QA_BENCHMARKING_DB_PUBLIC_READONLY TO ROLE useradmin; 
GRANT OWNERSHIP ON ROLE 			      			    _QA_BENCHMARKING_DB_PUBLIC_CRUD 	TO ROLE useradmin; 
GRANT ROLE 							      			_QA_BENCHMARKING_DB_PUBLIC_CRUD 	TO ROLE SYSADMIN	 ; 
GRANT ROLE 							      			_QA_BENCHMARKING_DB_PUBLIC_READONLY TO ROLE SYSADMIN     ; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA QA_BENCHMARKING_DB.PUBLIC TO ROLE _QA_BENCHMARKING_DB_PUBLIC_READONLY; 
GRANT READ 	 ON ALL STAGES 	  		      			 IN SCHEMA QA_BENCHMARKING_DB.PUBLIC TO ROLE _QA_BENCHMARKING_DB_PUBLIC_READONLY; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA QA_BENCHMARKING_DB.PUBLIC TO ROLE _QA_BENCHMARKING_DB_PUBLIC_READONLY; 
GRANT SELECT ON FUTURE TABLES 		      			     IN SCHEMA QA_BENCHMARKING_DB.PUBLIC TO ROLE _QA_BENCHMARKING_DB_PUBLIC_READONLY; 
GRANT SELECT ON ALL VIEWS 	  		      			     IN SCHEMA QA_BENCHMARKING_DB.PUBLIC TO ROLE _QA_BENCHMARKING_DB_PUBLIC_READONLY; 
GRANT SELECT ON FUTURE VIEWS  		      			     IN SCHEMA QA_BENCHMARKING_DB.PUBLIC TO ROLE _QA_BENCHMARKING_DB_PUBLIC_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA QA_BENCHMARKING_DB.PUBLIC TO ROLE _QA_BENCHMARKING_DB_PUBLIC_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA QA_BENCHMARKING_DB.PUBLIC TO ROLE _QA_BENCHMARKING_DB_PUBLIC_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA QA_BENCHMARKING_DB.PUBLIC TO ROLE _QA_BENCHMARKING_DB_PUBLIC_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA QA_BENCHMARKING_DB.PUBLIC TO ROLE _QA_BENCHMARKING_DB_PUBLIC_READONLY; 
GRANT SELECT ON ALL EXTERNAL TABLES       			     IN SCHEMA QA_BENCHMARKING_DB.PUBLIC TO ROLE _QA_BENCHMARKING_DB_PUBLIC_READONLY; 
GRANT SELECT ON FUTURE EXTERNAL TABLES 	  			 IN SCHEMA QA_BENCHMARKING_DB.PUBLIC TO ROLE _QA_BENCHMARKING_DB_PUBLIC_READONLY; 
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON ALL TABLES    IN SCHEMA QA_BENCHMARKING_DB.PUBLIC TO ROLE _QA_BENCHMARKING_DB_PUBLIC_CRUD;     
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON FUTURE TABLES IN SCHEMA QA_BENCHMARKING_DB.PUBLIC TO ROLE _QA_BENCHMARKING_DB_PUBLIC_CRUD;     
GRANT ROLE _QA_BENCHMARKING_DB_PUBLIC_READONLY to  role _QA_BENCHMARKING_DB_PUBLIC_CRUD; 
USE ROLE ACCOUNTADMIN; 
GRANT USAGE ON DATABASE QA_BENCHMARKING_DB to ROLE _QA_BENCHMARKING_DB_PUBLIC_READONLY; 
GRANT USAGE ON SCHEMA QA_BENCHMARKING_DB.PUBLIC TO ROLE _QA_BENCHMARKING_DB_PUBLIC_READONLY; 

USE ROLE SECURITYADMIN; 
CREATE ROLE IF NOT EXISTS			      			    _QA_BENCHMARKING_DB_EXPLORE_READONLY		  			     ; 
CREATE ROLE IF NOT EXISTS			      			    _QA_BENCHMARKING_DB_EXPLORE_CRUD						     ; 
GRANT OWNERSHIP ON ROLE 			      			    _QA_BENCHMARKING_DB_EXPLORE_READONLY TO ROLE useradmin; 
GRANT OWNERSHIP ON ROLE 			      			    _QA_BENCHMARKING_DB_EXPLORE_CRUD 	TO ROLE useradmin; 
GRANT ROLE 							      			_QA_BENCHMARKING_DB_EXPLORE_CRUD 	TO ROLE SYSADMIN	 ; 
GRANT ROLE 							      			_QA_BENCHMARKING_DB_EXPLORE_READONLY TO ROLE SYSADMIN     ; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA QA_BENCHMARKING_DB.EXPLORE TO ROLE _QA_BENCHMARKING_DB_EXPLORE_READONLY; 
GRANT READ 	 ON ALL STAGES 	  		      			 IN SCHEMA QA_BENCHMARKING_DB.EXPLORE TO ROLE _QA_BENCHMARKING_DB_EXPLORE_READONLY; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA QA_BENCHMARKING_DB.EXPLORE TO ROLE _QA_BENCHMARKING_DB_EXPLORE_READONLY; 
GRANT SELECT ON FUTURE TABLES 		      			     IN SCHEMA QA_BENCHMARKING_DB.EXPLORE TO ROLE _QA_BENCHMARKING_DB_EXPLORE_READONLY; 
GRANT SELECT ON ALL VIEWS 	  		      			     IN SCHEMA QA_BENCHMARKING_DB.EXPLORE TO ROLE _QA_BENCHMARKING_DB_EXPLORE_READONLY; 
GRANT SELECT ON FUTURE VIEWS  		      			     IN SCHEMA QA_BENCHMARKING_DB.EXPLORE TO ROLE _QA_BENCHMARKING_DB_EXPLORE_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA QA_BENCHMARKING_DB.EXPLORE TO ROLE _QA_BENCHMARKING_DB_EXPLORE_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA QA_BENCHMARKING_DB.EXPLORE TO ROLE _QA_BENCHMARKING_DB_EXPLORE_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA QA_BENCHMARKING_DB.EXPLORE TO ROLE _QA_BENCHMARKING_DB_EXPLORE_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA QA_BENCHMARKING_DB.EXPLORE TO ROLE _QA_BENCHMARKING_DB_EXPLORE_READONLY; 
GRANT SELECT ON ALL EXTERNAL TABLES       			     IN SCHEMA QA_BENCHMARKING_DB.EXPLORE TO ROLE _QA_BENCHMARKING_DB_EXPLORE_READONLY; 
GRANT SELECT ON FUTURE EXTERNAL TABLES 	  			 IN SCHEMA QA_BENCHMARKING_DB.EXPLORE TO ROLE _QA_BENCHMARKING_DB_EXPLORE_READONLY; 
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON ALL TABLES    IN SCHEMA QA_BENCHMARKING_DB.EXPLORE TO ROLE _QA_BENCHMARKING_DB_EXPLORE_CRUD;     
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON FUTURE TABLES IN SCHEMA QA_BENCHMARKING_DB.EXPLORE TO ROLE _QA_BENCHMARKING_DB_EXPLORE_CRUD;     
GRANT ROLE _QA_BENCHMARKING_DB_EXPLORE_READONLY to  role _QA_BENCHMARKING_DB_EXPLORE_CRUD; 
USE ROLE ACCOUNTADMIN; 
GRANT USAGE ON DATABASE QA_BENCHMARKING_DB to ROLE _QA_BENCHMARKING_DB_EXPLORE_READONLY; 
GRANT USAGE ON SCHEMA QA_BENCHMARKING_DB.EXPLORE TO ROLE _QA_BENCHMARKING_DB_EXPLORE_READONLY; 

USE ROLE SECURITYADMIN; 
CREATE ROLE IF NOT EXISTS			      			    _QA_BENCHMARKING_DB_PREP_READONLY		  			     ; 
CREATE ROLE IF NOT EXISTS			      			    _QA_BENCHMARKING_DB_PREP_CRUD						     ; 
GRANT OWNERSHIP ON ROLE 			      			    _QA_BENCHMARKING_DB_PREP_READONLY TO ROLE useradmin; 
GRANT OWNERSHIP ON ROLE 			      			    _QA_BENCHMARKING_DB_PREP_CRUD 	TO ROLE useradmin; 
GRANT ROLE 							      			_QA_BENCHMARKING_DB_PREP_CRUD 	TO ROLE SYSADMIN	 ; 
GRANT ROLE 							      			_QA_BENCHMARKING_DB_PREP_READONLY TO ROLE SYSADMIN     ; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA QA_BENCHMARKING_DB.PREP TO ROLE _QA_BENCHMARKING_DB_PREP_READONLY; 
GRANT READ 	 ON ALL STAGES 	  		      			 IN SCHEMA QA_BENCHMARKING_DB.PREP TO ROLE _QA_BENCHMARKING_DB_PREP_READONLY; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA QA_BENCHMARKING_DB.PREP TO ROLE _QA_BENCHMARKING_DB_PREP_READONLY; 
GRANT SELECT ON FUTURE TABLES 		      			     IN SCHEMA QA_BENCHMARKING_DB.PREP TO ROLE _QA_BENCHMARKING_DB_PREP_READONLY; 
GRANT SELECT ON ALL VIEWS 	  		      			     IN SCHEMA QA_BENCHMARKING_DB.PREP TO ROLE _QA_BENCHMARKING_DB_PREP_READONLY; 
GRANT SELECT ON FUTURE VIEWS  		      			     IN SCHEMA QA_BENCHMARKING_DB.PREP TO ROLE _QA_BENCHMARKING_DB_PREP_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA QA_BENCHMARKING_DB.PREP TO ROLE _QA_BENCHMARKING_DB_PREP_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA QA_BENCHMARKING_DB.PREP TO ROLE _QA_BENCHMARKING_DB_PREP_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA QA_BENCHMARKING_DB.PREP TO ROLE _QA_BENCHMARKING_DB_PREP_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA QA_BENCHMARKING_DB.PREP TO ROLE _QA_BENCHMARKING_DB_PREP_READONLY; 
GRANT SELECT ON ALL EXTERNAL TABLES       			     IN SCHEMA QA_BENCHMARKING_DB.PREP TO ROLE _QA_BENCHMARKING_DB_PREP_READONLY; 
GRANT SELECT ON FUTURE EXTERNAL TABLES 	  			 IN SCHEMA QA_BENCHMARKING_DB.PREP TO ROLE _QA_BENCHMARKING_DB_PREP_READONLY; 
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON ALL TABLES    IN SCHEMA QA_BENCHMARKING_DB.PREP TO ROLE _QA_BENCHMARKING_DB_PREP_CRUD;     
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON FUTURE TABLES IN SCHEMA QA_BENCHMARKING_DB.PREP TO ROLE _QA_BENCHMARKING_DB_PREP_CRUD;     
GRANT ROLE _QA_BENCHMARKING_DB_PREP_READONLY to  role _QA_BENCHMARKING_DB_PREP_CRUD; 
USE ROLE ACCOUNTADMIN; 
GRANT USAGE ON DATABASE QA_BENCHMARKING_DB to ROLE _QA_BENCHMARKING_DB_PREP_READONLY; 
GRANT USAGE ON SCHEMA QA_BENCHMARKING_DB.PREP TO ROLE _QA_BENCHMARKING_DB_PREP_READONLY; 

USE ROLE SECURITYADMIN; 
CREATE ROLE IF NOT EXISTS			      			    _QA_BENCHMARKING_DB_BASE_READONLY		  			     ; 
CREATE ROLE IF NOT EXISTS			      			    _QA_BENCHMARKING_DB_BASE_CRUD						     ; 
GRANT OWNERSHIP ON ROLE 			      			    _QA_BENCHMARKING_DB_BASE_READONLY TO ROLE useradmin; 
GRANT OWNERSHIP ON ROLE 			      			    _QA_BENCHMARKING_DB_BASE_CRUD 	TO ROLE useradmin; 
GRANT ROLE 							      			_QA_BENCHMARKING_DB_BASE_CRUD 	TO ROLE SYSADMIN	 ; 
GRANT ROLE 							      			_QA_BENCHMARKING_DB_BASE_READONLY TO ROLE SYSADMIN     ; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA QA_BENCHMARKING_DB.BASE TO ROLE _QA_BENCHMARKING_DB_BASE_READONLY; 
GRANT READ 	 ON ALL STAGES 	  		      			 IN SCHEMA QA_BENCHMARKING_DB.BASE TO ROLE _QA_BENCHMARKING_DB_BASE_READONLY; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA QA_BENCHMARKING_DB.BASE TO ROLE _QA_BENCHMARKING_DB_BASE_READONLY; 
GRANT SELECT ON FUTURE TABLES 		      			     IN SCHEMA QA_BENCHMARKING_DB.BASE TO ROLE _QA_BENCHMARKING_DB_BASE_READONLY; 
GRANT SELECT ON ALL VIEWS 	  		      			     IN SCHEMA QA_BENCHMARKING_DB.BASE TO ROLE _QA_BENCHMARKING_DB_BASE_READONLY; 
GRANT SELECT ON FUTURE VIEWS  		      			     IN SCHEMA QA_BENCHMARKING_DB.BASE TO ROLE _QA_BENCHMARKING_DB_BASE_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA QA_BENCHMARKING_DB.BASE TO ROLE _QA_BENCHMARKING_DB_BASE_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA QA_BENCHMARKING_DB.BASE TO ROLE _QA_BENCHMARKING_DB_BASE_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA QA_BENCHMARKING_DB.BASE TO ROLE _QA_BENCHMARKING_DB_BASE_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA QA_BENCHMARKING_DB.BASE TO ROLE _QA_BENCHMARKING_DB_BASE_READONLY; 
GRANT SELECT ON ALL EXTERNAL TABLES       			     IN SCHEMA QA_BENCHMARKING_DB.BASE TO ROLE _QA_BENCHMARKING_DB_BASE_READONLY; 
GRANT SELECT ON FUTURE EXTERNAL TABLES 	  			 IN SCHEMA QA_BENCHMARKING_DB.BASE TO ROLE _QA_BENCHMARKING_DB_BASE_READONLY; 
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON ALL TABLES    IN SCHEMA QA_BENCHMARKING_DB.BASE TO ROLE _QA_BENCHMARKING_DB_BASE_CRUD;     
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON FUTURE TABLES IN SCHEMA QA_BENCHMARKING_DB.BASE TO ROLE _QA_BENCHMARKING_DB_BASE_CRUD;     
GRANT ROLE _QA_BENCHMARKING_DB_BASE_READONLY to  role _QA_BENCHMARKING_DB_BASE_CRUD; 
USE ROLE ACCOUNTADMIN; 
GRANT USAGE ON DATABASE QA_BENCHMARKING_DB to ROLE _QA_BENCHMARKING_DB_BASE_READONLY; 
GRANT USAGE ON SCHEMA QA_BENCHMARKING_DB.BASE TO ROLE _QA_BENCHMARKING_DB_BASE_READONLY; 

USE ROLE SECURITYADMIN; 
CREATE ROLE IF NOT EXISTS			      			    _QA_BENCHMARKING_DB_ARCHIVE_READONLY		  			     ; 
CREATE ROLE IF NOT EXISTS			      			    _QA_BENCHMARKING_DB_ARCHIVE_CRUD						     ; 
GRANT OWNERSHIP ON ROLE 			      			    _QA_BENCHMARKING_DB_ARCHIVE_READONLY TO ROLE useradmin; 
GRANT OWNERSHIP ON ROLE 			      			    _QA_BENCHMARKING_DB_ARCHIVE_CRUD 	TO ROLE useradmin; 
GRANT ROLE 							      			_QA_BENCHMARKING_DB_ARCHIVE_CRUD 	TO ROLE SYSADMIN	 ; 
GRANT ROLE 							      			_QA_BENCHMARKING_DB_ARCHIVE_READONLY TO ROLE SYSADMIN     ; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA QA_BENCHMARKING_DB.ARCHIVE TO ROLE _QA_BENCHMARKING_DB_ARCHIVE_READONLY; 
GRANT READ 	 ON ALL STAGES 	  		      			 IN SCHEMA QA_BENCHMARKING_DB.ARCHIVE TO ROLE _QA_BENCHMARKING_DB_ARCHIVE_READONLY; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA QA_BENCHMARKING_DB.ARCHIVE TO ROLE _QA_BENCHMARKING_DB_ARCHIVE_READONLY; 
GRANT SELECT ON FUTURE TABLES 		      			     IN SCHEMA QA_BENCHMARKING_DB.ARCHIVE TO ROLE _QA_BENCHMARKING_DB_ARCHIVE_READONLY; 
GRANT SELECT ON ALL VIEWS 	  		      			     IN SCHEMA QA_BENCHMARKING_DB.ARCHIVE TO ROLE _QA_BENCHMARKING_DB_ARCHIVE_READONLY; 
GRANT SELECT ON FUTURE VIEWS  		      			     IN SCHEMA QA_BENCHMARKING_DB.ARCHIVE TO ROLE _QA_BENCHMARKING_DB_ARCHIVE_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA QA_BENCHMARKING_DB.ARCHIVE TO ROLE _QA_BENCHMARKING_DB_ARCHIVE_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA QA_BENCHMARKING_DB.ARCHIVE TO ROLE _QA_BENCHMARKING_DB_ARCHIVE_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA QA_BENCHMARKING_DB.ARCHIVE TO ROLE _QA_BENCHMARKING_DB_ARCHIVE_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA QA_BENCHMARKING_DB.ARCHIVE TO ROLE _QA_BENCHMARKING_DB_ARCHIVE_READONLY; 
GRANT SELECT ON ALL EXTERNAL TABLES       			     IN SCHEMA QA_BENCHMARKING_DB.ARCHIVE TO ROLE _QA_BENCHMARKING_DB_ARCHIVE_READONLY; 
GRANT SELECT ON FUTURE EXTERNAL TABLES 	  			 IN SCHEMA QA_BENCHMARKING_DB.ARCHIVE TO ROLE _QA_BENCHMARKING_DB_ARCHIVE_READONLY; 
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON ALL TABLES    IN SCHEMA QA_BENCHMARKING_DB.ARCHIVE TO ROLE _QA_BENCHMARKING_DB_ARCHIVE_CRUD;     
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON FUTURE TABLES IN SCHEMA QA_BENCHMARKING_DB.ARCHIVE TO ROLE _QA_BENCHMARKING_DB_ARCHIVE_CRUD;     
GRANT ROLE _QA_BENCHMARKING_DB_ARCHIVE_READONLY to  role _QA_BENCHMARKING_DB_ARCHIVE_CRUD; 
USE ROLE ACCOUNTADMIN; 
GRANT USAGE ON DATABASE QA_BENCHMARKING_DB to ROLE _QA_BENCHMARKING_DB_ARCHIVE_READONLY; 
GRANT USAGE ON SCHEMA QA_BENCHMARKING_DB.ARCHIVE TO ROLE _QA_BENCHMARKING_DB_ARCHIVE_READONLY; 

USE ROLE SECURITYADMIN; 
CREATE ROLE IF NOT EXISTS			      			    _DEV_CLAIM_DB_ETL_READONLY		  			     ; 
CREATE ROLE IF NOT EXISTS			      			    _DEV_CLAIM_DB_ETL_CRUD						     ; 
GRANT OWNERSHIP ON ROLE 			      			    _DEV_CLAIM_DB_ETL_READONLY TO ROLE useradmin; 
GRANT OWNERSHIP ON ROLE 			      			    _DEV_CLAIM_DB_ETL_CRUD 	TO ROLE useradmin; 
GRANT ROLE 							      			_DEV_CLAIM_DB_ETL_CRUD 	TO ROLE SYSADMIN	 ; 
GRANT ROLE 							      			_DEV_CLAIM_DB_ETL_READONLY TO ROLE SYSADMIN     ; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA DEV_CLAIM_DB.ETL TO ROLE _DEV_CLAIM_DB_ETL_READONLY; 
GRANT READ 	 ON ALL STAGES 	  		      			 IN SCHEMA DEV_CLAIM_DB.ETL TO ROLE _DEV_CLAIM_DB_ETL_READONLY; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA DEV_CLAIM_DB.ETL TO ROLE _DEV_CLAIM_DB_ETL_READONLY; 
GRANT SELECT ON FUTURE TABLES 		      			     IN SCHEMA DEV_CLAIM_DB.ETL TO ROLE _DEV_CLAIM_DB_ETL_READONLY; 
GRANT SELECT ON ALL VIEWS 	  		      			     IN SCHEMA DEV_CLAIM_DB.ETL TO ROLE _DEV_CLAIM_DB_ETL_READONLY; 
GRANT SELECT ON FUTURE VIEWS  		      			     IN SCHEMA DEV_CLAIM_DB.ETL TO ROLE _DEV_CLAIM_DB_ETL_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA DEV_CLAIM_DB.ETL TO ROLE _DEV_CLAIM_DB_ETL_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA DEV_CLAIM_DB.ETL TO ROLE _DEV_CLAIM_DB_ETL_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA DEV_CLAIM_DB.ETL TO ROLE _DEV_CLAIM_DB_ETL_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA DEV_CLAIM_DB.ETL TO ROLE _DEV_CLAIM_DB_ETL_READONLY; 
GRANT SELECT ON ALL EXTERNAL TABLES       			     IN SCHEMA DEV_CLAIM_DB.ETL TO ROLE _DEV_CLAIM_DB_ETL_READONLY; 
GRANT SELECT ON FUTURE EXTERNAL TABLES 	  			 IN SCHEMA DEV_CLAIM_DB.ETL TO ROLE _DEV_CLAIM_DB_ETL_READONLY; 
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON ALL TABLES    IN SCHEMA DEV_CLAIM_DB.ETL TO ROLE _DEV_CLAIM_DB_ETL_CRUD;     
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON FUTURE TABLES IN SCHEMA DEV_CLAIM_DB.ETL TO ROLE _DEV_CLAIM_DB_ETL_CRUD;     
GRANT ROLE _DEV_CLAIM_DB_ETL_READONLY to  role _DEV_CLAIM_DB_ETL_CRUD; 
USE ROLE ACCOUNTADMIN; 
GRANT USAGE ON DATABASE DEV_CLAIM_DB to ROLE _DEV_CLAIM_DB_ETL_READONLY; 
GRANT USAGE ON SCHEMA DEV_CLAIM_DB.ETL TO ROLE _DEV_CLAIM_DB_ETL_READONLY; 

