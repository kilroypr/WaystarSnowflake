USE ROLE SECURITYADMIN; 
CREATE ROLE IF NOT EXISTS			      			    _QA_DSCLAIMCLONE_DB_ETL_READONLY		  			     ; 
CREATE ROLE IF NOT EXISTS			      			    _QA_DSCLAIMCLONE_DB_ETL_CRUD						     ; 
GRANT OWNERSHIP ON ROLE 			      			    _QA_DSCLAIMCLONE_DB_ETL_READONLY TO ROLE useradmin; 
GRANT OWNERSHIP ON ROLE 			      			    _QA_DSCLAIMCLONE_DB_ETL_CRUD 	TO ROLE useradmin; 
GRANT ROLE 							      			_QA_DSCLAIMCLONE_DB_ETL_CRUD 	TO ROLE SYSADMIN	 ; 
GRANT ROLE 							      			_QA_DSCLAIMCLONE_DB_ETL_READONLY TO ROLE SYSADMIN     ; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA QA_DSCLAIMCLONE_DB.ETL TO ROLE _QA_DSCLAIMCLONE_DB_ETL_READONLY; 
GRANT READ 	 ON ALL STAGES 	  		      			 IN SCHEMA QA_DSCLAIMCLONE_DB.ETL TO ROLE _QA_DSCLAIMCLONE_DB_ETL_READONLY; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA QA_DSCLAIMCLONE_DB.ETL TO ROLE _QA_DSCLAIMCLONE_DB_ETL_READONLY; 
GRANT SELECT ON FUTURE TABLES 		      			     IN SCHEMA QA_DSCLAIMCLONE_DB.ETL TO ROLE _QA_DSCLAIMCLONE_DB_ETL_READONLY; 
GRANT SELECT ON ALL VIEWS 	  		      			     IN SCHEMA QA_DSCLAIMCLONE_DB.ETL TO ROLE _QA_DSCLAIMCLONE_DB_ETL_READONLY; 
GRANT SELECT ON FUTURE VIEWS  		      			     IN SCHEMA QA_DSCLAIMCLONE_DB.ETL TO ROLE _QA_DSCLAIMCLONE_DB_ETL_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA QA_DSCLAIMCLONE_DB.ETL TO ROLE _QA_DSCLAIMCLONE_DB_ETL_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA QA_DSCLAIMCLONE_DB.ETL TO ROLE _QA_DSCLAIMCLONE_DB_ETL_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA QA_DSCLAIMCLONE_DB.ETL TO ROLE _QA_DSCLAIMCLONE_DB_ETL_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA QA_DSCLAIMCLONE_DB.ETL TO ROLE _QA_DSCLAIMCLONE_DB_ETL_READONLY; 
GRANT SELECT ON ALL EXTERNAL TABLES       			     IN SCHEMA QA_DSCLAIMCLONE_DB.ETL TO ROLE _QA_DSCLAIMCLONE_DB_ETL_READONLY; 
GRANT SELECT ON FUTURE EXTERNAL TABLES 	  			 IN SCHEMA QA_DSCLAIMCLONE_DB.ETL TO ROLE _QA_DSCLAIMCLONE_DB_ETL_READONLY; 
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON ALL TABLES    IN SCHEMA QA_DSCLAIMCLONE_DB.ETL TO ROLE _QA_DSCLAIMCLONE_DB_ETL_CRUD;     
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON FUTURE TABLES IN SCHEMA QA_DSCLAIMCLONE_DB.ETL TO ROLE _QA_DSCLAIMCLONE_DB_ETL_CRUD;     
GRANT ROLE _QA_DSCLAIMCLONE_DB_ETL_READONLY to  role _QA_DSCLAIMCLONE_DB_ETL_CRUD; 
USE ROLE ACCOUNTADMIN; 
GRANT USAGE ON DATABASE QA_DSCLAIMCLONE_DB to ROLE _QA_DSCLAIMCLONE_DB_ETL_READONLY; 
GRANT USAGE ON SCHEMA QA_DSCLAIMCLONE_DB.ETL TO ROLE _QA_DSCLAIMCLONE_DB_ETL_READONLY; 

USE ROLE SECURITYADMIN; 
CREATE ROLE IF NOT EXISTS			      			    _QA_DSCLAIMCLONE_DB_EXPLORE_READONLY		  			     ; 
CREATE ROLE IF NOT EXISTS			      			    _QA_DSCLAIMCLONE_DB_EXPLORE_CRUD						     ; 
GRANT OWNERSHIP ON ROLE 			      			    _QA_DSCLAIMCLONE_DB_EXPLORE_READONLY TO ROLE useradmin; 
GRANT OWNERSHIP ON ROLE 			      			    _QA_DSCLAIMCLONE_DB_EXPLORE_CRUD 	TO ROLE useradmin; 
GRANT ROLE 							      			_QA_DSCLAIMCLONE_DB_EXPLORE_CRUD 	TO ROLE SYSADMIN	 ; 
GRANT ROLE 							      			_QA_DSCLAIMCLONE_DB_EXPLORE_READONLY TO ROLE SYSADMIN     ; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA QA_DSCLAIMCLONE_DB.EXPLORE TO ROLE _QA_DSCLAIMCLONE_DB_EXPLORE_READONLY; 
GRANT READ 	 ON ALL STAGES 	  		      			 IN SCHEMA QA_DSCLAIMCLONE_DB.EXPLORE TO ROLE _QA_DSCLAIMCLONE_DB_EXPLORE_READONLY; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA QA_DSCLAIMCLONE_DB.EXPLORE TO ROLE _QA_DSCLAIMCLONE_DB_EXPLORE_READONLY; 
GRANT SELECT ON FUTURE TABLES 		      			     IN SCHEMA QA_DSCLAIMCLONE_DB.EXPLORE TO ROLE _QA_DSCLAIMCLONE_DB_EXPLORE_READONLY; 
GRANT SELECT ON ALL VIEWS 	  		      			     IN SCHEMA QA_DSCLAIMCLONE_DB.EXPLORE TO ROLE _QA_DSCLAIMCLONE_DB_EXPLORE_READONLY; 
GRANT SELECT ON FUTURE VIEWS  		      			     IN SCHEMA QA_DSCLAIMCLONE_DB.EXPLORE TO ROLE _QA_DSCLAIMCLONE_DB_EXPLORE_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA QA_DSCLAIMCLONE_DB.EXPLORE TO ROLE _QA_DSCLAIMCLONE_DB_EXPLORE_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA QA_DSCLAIMCLONE_DB.EXPLORE TO ROLE _QA_DSCLAIMCLONE_DB_EXPLORE_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA QA_DSCLAIMCLONE_DB.EXPLORE TO ROLE _QA_DSCLAIMCLONE_DB_EXPLORE_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA QA_DSCLAIMCLONE_DB.EXPLORE TO ROLE _QA_DSCLAIMCLONE_DB_EXPLORE_READONLY; 
GRANT SELECT ON ALL EXTERNAL TABLES       			     IN SCHEMA QA_DSCLAIMCLONE_DB.EXPLORE TO ROLE _QA_DSCLAIMCLONE_DB_EXPLORE_READONLY; 
GRANT SELECT ON FUTURE EXTERNAL TABLES 	  			 IN SCHEMA QA_DSCLAIMCLONE_DB.EXPLORE TO ROLE _QA_DSCLAIMCLONE_DB_EXPLORE_READONLY; 
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON ALL TABLES    IN SCHEMA QA_DSCLAIMCLONE_DB.EXPLORE TO ROLE _QA_DSCLAIMCLONE_DB_EXPLORE_CRUD;     
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON FUTURE TABLES IN SCHEMA QA_DSCLAIMCLONE_DB.EXPLORE TO ROLE _QA_DSCLAIMCLONE_DB_EXPLORE_CRUD;     
GRANT ROLE _QA_DSCLAIMCLONE_DB_EXPLORE_READONLY to  role _QA_DSCLAIMCLONE_DB_EXPLORE_CRUD; 
USE ROLE ACCOUNTADMIN; 
GRANT USAGE ON DATABASE QA_DSCLAIMCLONE_DB to ROLE _QA_DSCLAIMCLONE_DB_EXPLORE_READONLY; 
GRANT USAGE ON SCHEMA QA_DSCLAIMCLONE_DB.EXPLORE TO ROLE _QA_DSCLAIMCLONE_DB_EXPLORE_READONLY; 

USE ROLE SECURITYADMIN; 
CREATE ROLE IF NOT EXISTS			      			    _QA_DSCLAIMCLONE_DB_BASE_READONLY		  			     ; 
CREATE ROLE IF NOT EXISTS			      			    _QA_DSCLAIMCLONE_DB_BASE_CRUD						     ; 
GRANT OWNERSHIP ON ROLE 			      			    _QA_DSCLAIMCLONE_DB_BASE_READONLY TO ROLE useradmin; 
GRANT OWNERSHIP ON ROLE 			      			    _QA_DSCLAIMCLONE_DB_BASE_CRUD 	TO ROLE useradmin; 
GRANT ROLE 							      			_QA_DSCLAIMCLONE_DB_BASE_CRUD 	TO ROLE SYSADMIN	 ; 
GRANT ROLE 							      			_QA_DSCLAIMCLONE_DB_BASE_READONLY TO ROLE SYSADMIN     ; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA QA_DSCLAIMCLONE_DB.BASE TO ROLE _QA_DSCLAIMCLONE_DB_BASE_READONLY; 
GRANT READ 	 ON ALL STAGES 	  		      			 IN SCHEMA QA_DSCLAIMCLONE_DB.BASE TO ROLE _QA_DSCLAIMCLONE_DB_BASE_READONLY; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA QA_DSCLAIMCLONE_DB.BASE TO ROLE _QA_DSCLAIMCLONE_DB_BASE_READONLY; 
GRANT SELECT ON FUTURE TABLES 		      			     IN SCHEMA QA_DSCLAIMCLONE_DB.BASE TO ROLE _QA_DSCLAIMCLONE_DB_BASE_READONLY; 
GRANT SELECT ON ALL VIEWS 	  		      			     IN SCHEMA QA_DSCLAIMCLONE_DB.BASE TO ROLE _QA_DSCLAIMCLONE_DB_BASE_READONLY; 
GRANT SELECT ON FUTURE VIEWS  		      			     IN SCHEMA QA_DSCLAIMCLONE_DB.BASE TO ROLE _QA_DSCLAIMCLONE_DB_BASE_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA QA_DSCLAIMCLONE_DB.BASE TO ROLE _QA_DSCLAIMCLONE_DB_BASE_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA QA_DSCLAIMCLONE_DB.BASE TO ROLE _QA_DSCLAIMCLONE_DB_BASE_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA QA_DSCLAIMCLONE_DB.BASE TO ROLE _QA_DSCLAIMCLONE_DB_BASE_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA QA_DSCLAIMCLONE_DB.BASE TO ROLE _QA_DSCLAIMCLONE_DB_BASE_READONLY; 
GRANT SELECT ON ALL EXTERNAL TABLES       			     IN SCHEMA QA_DSCLAIMCLONE_DB.BASE TO ROLE _QA_DSCLAIMCLONE_DB_BASE_READONLY; 
GRANT SELECT ON FUTURE EXTERNAL TABLES 	  			 IN SCHEMA QA_DSCLAIMCLONE_DB.BASE TO ROLE _QA_DSCLAIMCLONE_DB_BASE_READONLY; 
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON ALL TABLES    IN SCHEMA QA_DSCLAIMCLONE_DB.BASE TO ROLE _QA_DSCLAIMCLONE_DB_BASE_CRUD;     
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON FUTURE TABLES IN SCHEMA QA_DSCLAIMCLONE_DB.BASE TO ROLE _QA_DSCLAIMCLONE_DB_BASE_CRUD;     
GRANT ROLE _QA_DSCLAIMCLONE_DB_BASE_READONLY to  role _QA_DSCLAIMCLONE_DB_BASE_CRUD; 
USE ROLE ACCOUNTADMIN; 
GRANT USAGE ON DATABASE QA_DSCLAIMCLONE_DB to ROLE _QA_DSCLAIMCLONE_DB_BASE_READONLY; 
GRANT USAGE ON SCHEMA QA_DSCLAIMCLONE_DB.BASE TO ROLE _QA_DSCLAIMCLONE_DB_BASE_READONLY; 

