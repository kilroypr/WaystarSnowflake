USE ROLE SECURITYADMIN; 
CREATE ROLE IF NOT EXISTS			      			    _DEV_CLAIM_DB_BASE_READONLY		  			     ; 
CREATE ROLE IF NOT EXISTS			      			    _DEV_CLAIM_DB_BASE_CRUD						     ; 
