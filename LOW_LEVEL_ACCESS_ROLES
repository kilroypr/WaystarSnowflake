GRANT OWNERSHIP ON ROLE 			      			    _DEV_DENIALS_DB_LOOKER_SCRATCH_READONLY TO ROLE useradmin; 
GRANT OWNERSHIP ON ROLE 			      			    _DEV_DENIALS_DB_LOOKER_SCRATCH_CRUD 	TO ROLE useradmin; 
GRANT ROLE 							      			_DEV_DENIALS_DB_LOOKER_SCRATCH_CRUD 	TO ROLE SYSADMIN	 ; 
GRANT ROLE 							      			_DEV_DENIALS_DB_LOOKER_SCRATCH_READONLY TO ROLE SYSADMIN     ; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA DEV_DENIALS_DB.LOOKER_SCRATCH TO ROLE _DEV_DENIALS_DB_LOOKER_SCRATCH_READONLY; 
GRANT READ 	 ON ALL STAGES 	  		      			 IN SCHEMA DEV_DENIALS_DB.LOOKER_SCRATCH TO ROLE _DEV_DENIALS_DB_LOOKER_SCRATCH_READONLY; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA DEV_DENIALS_DB.LOOKER_SCRATCH TO ROLE _DEV_DENIALS_DB_LOOKER_SCRATCH_READONLY; 
GRANT SELECT ON FUTURE TABLES 		      			     IN SCHEMA DEV_DENIALS_DB.LOOKER_SCRATCH TO ROLE _DEV_DENIALS_DB_LOOKER_SCRATCH_READONLY; 
GRANT SELECT ON ALL VIEWS 	  		      			     IN SCHEMA DEV_DENIALS_DB.LOOKER_SCRATCH TO ROLE _DEV_DENIALS_DB_LOOKER_SCRATCH_READONLY; 
GRANT SELECT ON FUTURE VIEWS  		      			     IN SCHEMA DEV_DENIALS_DB.LOOKER_SCRATCH TO ROLE _DEV_DENIALS_DB_LOOKER_SCRATCH_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA DEV_DENIALS_DB.LOOKER_SCRATCH TO ROLE _DEV_DENIALS_DB_LOOKER_SCRATCH_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA DEV_DENIALS_DB.LOOKER_SCRATCH TO ROLE _DEV_DENIALS_DB_LOOKER_SCRATCH_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA DEV_DENIALS_DB.LOOKER_SCRATCH TO ROLE _DEV_DENIALS_DB_LOOKER_SCRATCH_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA DEV_DENIALS_DB.LOOKER_SCRATCH TO ROLE _DEV_DENIALS_DB_LOOKER_SCRATCH_READONLY; 
GRANT SELECT ON ALL EXTERNAL TABLES       			     IN SCHEMA DEV_DENIALS_DB.LOOKER_SCRATCH TO ROLE _DEV_DENIALS_DB_LOOKER_SCRATCH_READONLY; 
GRANT SELECT ON FUTURE EXTERNAL TABLES 	  			 IN SCHEMA DEV_DENIALS_DB.LOOKER_SCRATCH TO ROLE _DEV_DENIALS_DB_LOOKER_SCRATCH_READONLY; 
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON ALL TABLES    IN SCHEMA DEV_DENIALS_DB.LOOKER_SCRATCH TO ROLE _DEV_DENIALS_DB_LOOKER_SCRATCH_CRUD;     
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON FUTURE TABLES IN SCHEMA DEV_DENIALS_DB.LOOKER_SCRATCH TO ROLE _DEV_DENIALS_DB_LOOKER_SCRATCH_CRUD;     
GRANT ROLE _DEV_DENIALS_DB_LOOKER_SCRATCH_READONLY to  role _DEV_DENIALS_DB_LOOKER_SCRATCH_CRUD; 
USE ROLE ACCOUNTADMIN; 
GRANT USAGE ON DATABASE DEV_DENIALS_DB to ROLE _DEV_DENIALS_DB_LOOKER_SCRATCH_READONLY; 
GRANT USAGE ON SCHEMA DEV_DENIALS_DB.LOOKER_SCRATCH TO ROLE _DEV_DENIALS_DB_LOOKER_SCRATCH_READONLY; 

