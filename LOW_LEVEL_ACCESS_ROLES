GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA DEV_LOGVIEWER_DB.PUBLIC TO ROLE _DEV_LOGVIEWER_DB_PUBLIC_READONLY; 
GRANT READ 	 ON ALL STAGES 	  		      			 IN SCHEMA DEV_LOGVIEWER_DB.PUBLIC TO ROLE _DEV_LOGVIEWER_DB_PUBLIC_READONLY; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA DEV_LOGVIEWER_DB.PUBLIC TO ROLE _DEV_LOGVIEWER_DB_PUBLIC_READONLY; 
GRANT SELECT ON FUTURE TABLES 		      			     IN SCHEMA DEV_LOGVIEWER_DB.PUBLIC TO ROLE _DEV_LOGVIEWER_DB_PUBLIC_READONLY; 
GRANT SELECT ON ALL VIEWS 	  		      			     IN SCHEMA DEV_LOGVIEWER_DB.PUBLIC TO ROLE _DEV_LOGVIEWER_DB_PUBLIC_READONLY; 
GRANT SELECT ON FUTURE VIEWS  		      			     IN SCHEMA DEV_LOGVIEWER_DB.PUBLIC TO ROLE _DEV_LOGVIEWER_DB_PUBLIC_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA DEV_LOGVIEWER_DB.PUBLIC TO ROLE _DEV_LOGVIEWER_DB_PUBLIC_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA DEV_LOGVIEWER_DB.PUBLIC TO ROLE _DEV_LOGVIEWER_DB_PUBLIC_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA DEV_LOGVIEWER_DB.PUBLIC TO ROLE _DEV_LOGVIEWER_DB_PUBLIC_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA DEV_LOGVIEWER_DB.PUBLIC TO ROLE _DEV_LOGVIEWER_DB_PUBLIC_READONLY; 
GRANT SELECT ON ALL EXTERNAL TABLES       			     IN SCHEMA DEV_LOGVIEWER_DB.PUBLIC TO ROLE _DEV_LOGVIEWER_DB_PUBLIC_READONLY; 
GRANT SELECT ON FUTURE EXTERNAL TABLES 	  			 IN SCHEMA DEV_LOGVIEWER_DB.PUBLIC TO ROLE _DEV_LOGVIEWER_DB_PUBLIC_READONLY; 
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON ALL TABLES    IN SCHEMA DEV_LOGVIEWER_DB.PUBLIC TO ROLE _DEV_LOGVIEWER_DB_PUBLIC_CRUD;     
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON FUTURE TABLES IN SCHEMA DEV_LOGVIEWER_DB.PUBLIC TO ROLE _DEV_LOGVIEWER_DB_PUBLIC_CRUD;     
GRANT ROLE _DEV_LOGVIEWER_DB_PUBLIC_READONLY to  role _DEV_LOGVIEWER_DB_PUBLIC_CRUD; 
USE ROLE ACCOUNTADMIN; 
GRANT USAGE ON DATABASE DEV_LOGVIEWER_DB to ROLE _DEV_LOGVIEWER_DB_PUBLIC_READONLY; 
GRANT USAGE ON SCHEMA DEV_LOGVIEWER_DB.PUBLIC TO ROLE _DEV_LOGVIEWER_DB_PUBLIC_READONLY; 

USE ROLE SECURITYADMIN; 
CREATE ROLE IF NOT EXISTS			      			    _DEV_LOGVIEWER_DB_BASE_READONLY		  			     ; 
CREATE ROLE IF NOT EXISTS			      			    _DEV_LOGVIEWER_DB_BASE_CRUD						     ; 
GRANT OWNERSHIP ON ROLE 			      			    _DEV_LOGVIEWER_DB_BASE_READONLY TO ROLE useradmin; 
GRANT OWNERSHIP ON ROLE 			      			    _DEV_LOGVIEWER_DB_BASE_CRUD 	TO ROLE useradmin; 
