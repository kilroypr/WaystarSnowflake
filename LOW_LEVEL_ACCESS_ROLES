USE ROLE SECURITYADMIN; 
CREATE ROLE IF NOT EXISTS			      			    _DEV_LOGVIEWER_DB_ARCHIVE_READONLY		  			     ; 
CREATE ROLE IF NOT EXISTS			      			    _DEV_LOGVIEWER_DB_ARCHIVE_CRUD						     ; 
GRANT OWNERSHIP ON ROLE 			      			    _DEV_LOGVIEWER_DB_ARCHIVE_READONLY TO ROLE useradmin; 
GRANT OWNERSHIP ON ROLE 			      			    _DEV_LOGVIEWER_DB_ARCHIVE_CRUD 	TO ROLE useradmin; 
GRANT ROLE 							      			_DEV_LOGVIEWER_DB_ARCHIVE_CRUD 	TO ROLE SYSADMIN	 ; 
GRANT ROLE 							      			_DEV_LOGVIEWER_DB_ARCHIVE_READONLY TO ROLE SYSADMIN     ; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA DEV_LOGVIEWER_DB.ARCHIVE TO ROLE _DEV_LOGVIEWER_DB_ARCHIVE_READONLY; 
GRANT READ 	 ON ALL STAGES 	  		      			 IN SCHEMA DEV_LOGVIEWER_DB.ARCHIVE TO ROLE _DEV_LOGVIEWER_DB_ARCHIVE_READONLY; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA DEV_LOGVIEWER_DB.ARCHIVE TO ROLE _DEV_LOGVIEWER_DB_ARCHIVE_READONLY; 
GRANT SELECT ON FUTURE TABLES 		      			     IN SCHEMA DEV_LOGVIEWER_DB.ARCHIVE TO ROLE _DEV_LOGVIEWER_DB_ARCHIVE_READONLY; 
GRANT SELECT ON ALL VIEWS 	  		      			     IN SCHEMA DEV_LOGVIEWER_DB.ARCHIVE TO ROLE _DEV_LOGVIEWER_DB_ARCHIVE_READONLY; 
GRANT SELECT ON FUTURE VIEWS  		      			     IN SCHEMA DEV_LOGVIEWER_DB.ARCHIVE TO ROLE _DEV_LOGVIEWER_DB_ARCHIVE_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA DEV_LOGVIEWER_DB.ARCHIVE TO ROLE _DEV_LOGVIEWER_DB_ARCHIVE_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA DEV_LOGVIEWER_DB.ARCHIVE TO ROLE _DEV_LOGVIEWER_DB_ARCHIVE_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA DEV_LOGVIEWER_DB.ARCHIVE TO ROLE _DEV_LOGVIEWER_DB_ARCHIVE_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA DEV_LOGVIEWER_DB.ARCHIVE TO ROLE _DEV_LOGVIEWER_DB_ARCHIVE_READONLY; 
GRANT SELECT ON ALL EXTERNAL TABLES       			     IN SCHEMA DEV_LOGVIEWER_DB.ARCHIVE TO ROLE _DEV_LOGVIEWER_DB_ARCHIVE_READONLY; 
GRANT SELECT ON FUTURE EXTERNAL TABLES 	  			 IN SCHEMA DEV_LOGVIEWER_DB.ARCHIVE TO ROLE _DEV_LOGVIEWER_DB_ARCHIVE_READONLY; 
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON ALL TABLES    IN SCHEMA DEV_LOGVIEWER_DB.ARCHIVE TO ROLE _DEV_LOGVIEWER_DB_ARCHIVE_CRUD;     
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON FUTURE TABLES IN SCHEMA DEV_LOGVIEWER_DB.ARCHIVE TO ROLE _DEV_LOGVIEWER_DB_ARCHIVE_CRUD;     
GRANT ROLE _DEV_LOGVIEWER_DB_ARCHIVE_READONLY to  role _DEV_LOGVIEWER_DB_ARCHIVE_CRUD; 
USE ROLE ACCOUNTADMIN; 
GRANT USAGE ON DATABASE DEV_LOGVIEWER_DB to ROLE _DEV_LOGVIEWER_DB_ARCHIVE_READONLY; 
GRANT USAGE ON SCHEMA DEV_LOGVIEWER_DB.ARCHIVE TO ROLE _DEV_LOGVIEWER_DB_ARCHIVE_READONLY; 

USE ROLE SECURITYADMIN; 
CREATE ROLE IF NOT EXISTS			      			    _DEV_LOGVIEWER_DB_ETL_READONLY		  			     ; 
CREATE ROLE IF NOT EXISTS			      			    _DEV_LOGVIEWER_DB_ETL_CRUD						     ; 
GRANT OWNERSHIP ON ROLE 			      			    _DEV_LOGVIEWER_DB_ETL_READONLY TO ROLE useradmin; 
GRANT OWNERSHIP ON ROLE 			      			    _DEV_LOGVIEWER_DB_ETL_CRUD 	TO ROLE useradmin; 
GRANT ROLE 							      			_DEV_LOGVIEWER_DB_ETL_CRUD 	TO ROLE SYSADMIN	 ; 
GRANT ROLE 							      			_DEV_LOGVIEWER_DB_ETL_READONLY TO ROLE SYSADMIN     ; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA DEV_LOGVIEWER_DB.ETL TO ROLE _DEV_LOGVIEWER_DB_ETL_READONLY; 
GRANT READ 	 ON ALL STAGES 	  		      			 IN SCHEMA DEV_LOGVIEWER_DB.ETL TO ROLE _DEV_LOGVIEWER_DB_ETL_READONLY; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA DEV_LOGVIEWER_DB.ETL TO ROLE _DEV_LOGVIEWER_DB_ETL_READONLY; 
GRANT SELECT ON FUTURE TABLES 		      			     IN SCHEMA DEV_LOGVIEWER_DB.ETL TO ROLE _DEV_LOGVIEWER_DB_ETL_READONLY; 
GRANT SELECT ON ALL VIEWS 	  		      			     IN SCHEMA DEV_LOGVIEWER_DB.ETL TO ROLE _DEV_LOGVIEWER_DB_ETL_READONLY; 
GRANT SELECT ON FUTURE VIEWS  		      			     IN SCHEMA DEV_LOGVIEWER_DB.ETL TO ROLE _DEV_LOGVIEWER_DB_ETL_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA DEV_LOGVIEWER_DB.ETL TO ROLE _DEV_LOGVIEWER_DB_ETL_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA DEV_LOGVIEWER_DB.ETL TO ROLE _DEV_LOGVIEWER_DB_ETL_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA DEV_LOGVIEWER_DB.ETL TO ROLE _DEV_LOGVIEWER_DB_ETL_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA DEV_LOGVIEWER_DB.ETL TO ROLE _DEV_LOGVIEWER_DB_ETL_READONLY; 
GRANT SELECT ON ALL EXTERNAL TABLES       			     IN SCHEMA DEV_LOGVIEWER_DB.ETL TO ROLE _DEV_LOGVIEWER_DB_ETL_READONLY; 
GRANT SELECT ON FUTURE EXTERNAL TABLES 	  			 IN SCHEMA DEV_LOGVIEWER_DB.ETL TO ROLE _DEV_LOGVIEWER_DB_ETL_READONLY; 
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON ALL TABLES    IN SCHEMA DEV_LOGVIEWER_DB.ETL TO ROLE _DEV_LOGVIEWER_DB_ETL_CRUD;     
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON FUTURE TABLES IN SCHEMA DEV_LOGVIEWER_DB.ETL TO ROLE _DEV_LOGVIEWER_DB_ETL_CRUD;     
GRANT ROLE _DEV_LOGVIEWER_DB_ETL_READONLY to  role _DEV_LOGVIEWER_DB_ETL_CRUD; 
USE ROLE ACCOUNTADMIN; 
GRANT USAGE ON DATABASE DEV_LOGVIEWER_DB to ROLE _DEV_LOGVIEWER_DB_ETL_READONLY; 
GRANT USAGE ON SCHEMA DEV_LOGVIEWER_DB.ETL TO ROLE _DEV_LOGVIEWER_DB_ETL_READONLY; 

USE ROLE SECURITYADMIN; 
CREATE ROLE IF NOT EXISTS			      			    _DEV_LOGVIEWER_DB_PUBLIC_READONLY		  			     ; 
CREATE ROLE IF NOT EXISTS			      			    _DEV_LOGVIEWER_DB_PUBLIC_CRUD						     ; 
