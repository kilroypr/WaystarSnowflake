GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA DEV_LOGVIEWER_DB.BASE TO ROLE _DEV_LOGVIEWER_DB_BASE_READONLY; 
GRANT READ 	 ON ALL STAGES 	  		      			 IN SCHEMA DEV_LOGVIEWER_DB.BASE TO ROLE _DEV_LOGVIEWER_DB_BASE_READONLY; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA DEV_LOGVIEWER_DB.BASE TO ROLE _DEV_LOGVIEWER_DB_BASE_READONLY; 
GRANT SELECT ON FUTURE TABLES 		      			     IN SCHEMA DEV_LOGVIEWER_DB.BASE TO ROLE _DEV_LOGVIEWER_DB_BASE_READONLY; 
GRANT SELECT ON ALL VIEWS 	  		      			     IN SCHEMA DEV_LOGVIEWER_DB.BASE TO ROLE _DEV_LOGVIEWER_DB_BASE_READONLY; 
GRANT SELECT ON FUTURE VIEWS  		      			     IN SCHEMA DEV_LOGVIEWER_DB.BASE TO ROLE _DEV_LOGVIEWER_DB_BASE_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA DEV_LOGVIEWER_DB.BASE TO ROLE _DEV_LOGVIEWER_DB_BASE_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA DEV_LOGVIEWER_DB.BASE TO ROLE _DEV_LOGVIEWER_DB_BASE_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA DEV_LOGVIEWER_DB.BASE TO ROLE _DEV_LOGVIEWER_DB_BASE_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA DEV_LOGVIEWER_DB.BASE TO ROLE _DEV_LOGVIEWER_DB_BASE_READONLY; 
GRANT SELECT ON ALL EXTERNAL TABLES       			     IN SCHEMA DEV_LOGVIEWER_DB.BASE TO ROLE _DEV_LOGVIEWER_DB_BASE_READONLY; 
GRANT SELECT ON FUTURE EXTERNAL TABLES 	  			 IN SCHEMA DEV_LOGVIEWER_DB.BASE TO ROLE _DEV_LOGVIEWER_DB_BASE_READONLY; 
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON ALL TABLES    IN SCHEMA DEV_LOGVIEWER_DB.BASE TO ROLE _DEV_LOGVIEWER_DB_BASE_CRUD;     
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON FUTURE TABLES IN SCHEMA DEV_LOGVIEWER_DB.BASE TO ROLE _DEV_LOGVIEWER_DB_BASE_CRUD;     
GRANT ROLE _DEV_LOGVIEWER_DB_BASE_READONLY to  role _DEV_LOGVIEWER_DB_BASE_CRUD; 
