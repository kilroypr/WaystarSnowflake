GRANT OWNERSHIP ON ROLE 			      			    _PR_DENIALS_DB_EXPLORE_CRUD 	TO ROLE useradmin; 
GRANT ROLE 							      			_PR_DENIALS_DB_EXPLORE_CRUD 	TO ROLE SYSADMIN	 ; 
GRANT ROLE 							      			_PR_DENIALS_DB_EXPLORE_READONLY TO ROLE SYSADMIN     ; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA PR_DENIALS_DB.EXPLORE TO ROLE _PR_DENIALS_DB_EXPLORE_READONLY; 
GRANT READ 	 ON ALL STAGES 	  		      			 IN SCHEMA PR_DENIALS_DB.EXPLORE TO ROLE _PR_DENIALS_DB_EXPLORE_READONLY; 
GRANT SELECT ON ALL TABLES 	  		      			 IN SCHEMA PR_DENIALS_DB.EXPLORE TO ROLE _PR_DENIALS_DB_EXPLORE_READONLY; 
GRANT SELECT ON FUTURE TABLES 		      			     IN SCHEMA PR_DENIALS_DB.EXPLORE TO ROLE _PR_DENIALS_DB_EXPLORE_READONLY; 
GRANT SELECT ON ALL VIEWS 	  		      			     IN SCHEMA PR_DENIALS_DB.EXPLORE TO ROLE _PR_DENIALS_DB_EXPLORE_READONLY; 
GRANT SELECT ON FUTURE VIEWS  		      			     IN SCHEMA PR_DENIALS_DB.EXPLORE TO ROLE _PR_DENIALS_DB_EXPLORE_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA PR_DENIALS_DB.EXPLORE TO ROLE _PR_DENIALS_DB_EXPLORE_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA PR_DENIALS_DB.EXPLORE TO ROLE _PR_DENIALS_DB_EXPLORE_READONLY; 
GRANT SELECT ON ALL MATERIALIZED VIEWS    			     IN SCHEMA PR_DENIALS_DB.EXPLORE TO ROLE _PR_DENIALS_DB_EXPLORE_READONLY; 
GRANT SELECT ON FUTURE MATERIALIZED VIEWS 			     IN SCHEMA PR_DENIALS_DB.EXPLORE TO ROLE _PR_DENIALS_DB_EXPLORE_READONLY; 
GRANT SELECT ON ALL EXTERNAL TABLES       			     IN SCHEMA PR_DENIALS_DB.EXPLORE TO ROLE _PR_DENIALS_DB_EXPLORE_READONLY; 
GRANT SELECT ON FUTURE EXTERNAL TABLES 	  			 IN SCHEMA PR_DENIALS_DB.EXPLORE TO ROLE _PR_DENIALS_DB_EXPLORE_READONLY; 
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON ALL TABLES    IN SCHEMA PR_DENIALS_DB.EXPLORE TO ROLE _PR_DENIALS_DB_EXPLORE_CRUD;     
GRANT INSERT, UPDATE, DELETE, TRUNCATE ON FUTURE TABLES IN SCHEMA PR_DENIALS_DB.EXPLORE TO ROLE _PR_DENIALS_DB_EXPLORE_CRUD;     
GRANT ROLE _PR_DENIALS_DB_EXPLORE_READONLY to  role _PR_DENIALS_DB_EXPLORE_CRUD; 
USE ROLE ACCOUNTADMIN; 
GRANT USAGE ON DATABASE PR_DENIALS_DB to ROLE _PR_DENIALS_DB_EXPLORE_READONLY; 
GRANT USAGE ON SCHEMA PR_DENIALS_DB.EXPLORE TO ROLE _PR_DENIALS_DB_EXPLORE_READONLY; 

